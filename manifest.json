{
  "name": "Chrome Bookmark Categorizer",
  "description": "Built for people who have disorder of opening 1000s of tabs and then complaining why the fck my laptop is slow!!!",
  "version": "1.0",
  "icons": { "16": "./icons/icon16.png", "48": "./icons/icon48.png", "128": "./icons/icon128.png" },
  "manifest_version": 3,
  "background": {
      "service_worker": "background.js"
  },
  "permissions": ["storage", "tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/icons/icon16.png",
      "48": "/icons/icon48.png",
      "128": "/icons/icon128.png"
    }
  }
}
